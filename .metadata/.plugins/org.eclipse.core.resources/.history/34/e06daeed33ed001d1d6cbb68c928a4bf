import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		float x = 10.4555666f;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite seu nome: ");
		String nome = sc.next();
		System.out.println("Ol√° " + nome + "!");
		System.out.printf("%.2f", x);
		sc.close();
	}

}
