import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		float x = 10.4555666f;
		
		System.out.printf("%.4f", x);
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite seu nome: ");
		String nome = sc.next();
		System.out.println("Ol√° " + nome + "!");
		sc.close();
	}

}
